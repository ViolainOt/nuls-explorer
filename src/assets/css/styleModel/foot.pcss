@import "pcsscommont.pcss";

footer{
  margin-top:120px;
  background:#0c0e12;
  width:100%;
  & .nuls-footer{
    width: var(--content-width);
    margin:0 auto;
    height: 80px;
    line-height: 80px;
    text-align: center;
    &> span{
      margin-right:50px;
      display:inline-block;
      vertical-align: middle;
      & a{
        display:inline-block;
        vertical-align: middle;
        & i{
          vertical-align: middle;
        }
      }
    }
  }
}


